#include <stdio.h>

int main(){
    int n; printf("Rows: "); scanf("%d",&n);
    for(int i=0;i<n;i++){
        unsigned long long c = 1;
        for(int s=0;s<n-i-1;s++) putchar(' ');
        for(int j=0;j<=i;j++){
            printf("%llu ", c);
            c = c * (i - j) / (j + 1); // next binomial coefficient
        }
        putchar('\n');
    }
    return 0;
}
